{
    "name": "abdugoffor/apidebug",
    "description": "A middleware for debugging and profiling API requests for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ApiMiddleware\\ApiDebug\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApiMiddleware\\ApiDebug\\ApiDebugServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Abdugoffor/debug.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}